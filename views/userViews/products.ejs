                    <button type="button" class="btn btn-light" onclick="location.href='/logIn';">Log In</button>
                    <button type="button" class="btn btn-light" onclick="location.href='/signUp';">Sign Up</button>
                </div>
                <div class="modal-footer">
                    <button type="button" id="closeNonUserModal" class="btn btn-light"
                        data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Remove Product Success Modal -->
    <div class="modal fade" id="removeSuccessModal" tabindex="-1" role="dialog"
