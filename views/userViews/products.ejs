                                <li>
                                    <a>Price</a>
                                    <form action="#" method="POST">
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="2000-3000" name="price-range">
                                                <label class="custom-control-label" for="2000-3000">2000 - 3000</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="3000-4000" name="price-range">
                                                <label class="custom-control-label" for="3000-4000">3000 - 4000</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="5000-7000" name="price-range">
                                                <label class="custom-control-label" for="5000-7000">5000 - 7000</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
