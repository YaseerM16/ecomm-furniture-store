    <!-- Sign Up/Login Modal -->
    <div class="modal fade" id="signupLoginModal" tabindex="-1" role="dialog" aria-labelledby="signupLoginModalLabel"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content" style="background-color: #8b6d32a8;">
                <div class="modal-header">
                    <h5 class="modal-title text-white" id="signupLoginModalLabel">Sign Up/Login</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
