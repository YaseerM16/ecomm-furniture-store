                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="name-descending" name="sort">
                                                <label class="custom-control-label" for="name-descending">Name
                                                    descending</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="price-ascending" name="sort">
                                                <label class="custom-control-label" for="price-ascending">Price
                                                    ascending</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="price-descending" name="sort">
                                                <label class="custom-control-label" for="price-descending">Price
                                                    descending</label>
                                            </div>
                                        </div>
                                    </form>
                                </li>
                            </ul>
                            <div class="section d-flex justify-content-center flex-column" style="margin-right: 67px;">
                                <div class="d-flex justify-content-center flex-column">

                                    <button id="apply-filters" class="btn btn-primary"
                                        style="font-size: 1.2em; padding: 0.5em 1em;">Apply filters</button>
                                    <button id="remove-filters" class="btn btn-secondary">Remove filters</button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-xl-9 col-lg-8" style="margin-top: 80px;">
                        <div class="section-header">
                            <h3 class="title">Products</h3>
                            <p class="text">Here are there unique products</p>
                        </div>
                        <div class="row">
                            <!-- Item -->
                            <% for(let i=0; i<productDet.length; i++){ %>
                                <div class="col-lg-4 col-sm-6">
                                    <div class="product_block">
                                        <div class="product_top">
                                            <div>
                                                <% if (productDet[i].productOfferPercentage ) { %>
                                                    <span class="product_tag">
                                                        <%=productDet[i].productOfferPercentage%>% Off
                                                    </span>
                                                    <% } else { %>
                                                        <% } %>

                                            </div>
                                            <div>
                                                <% if (productDet[i].isWishListed ) { %>
                                                    <button type="button" class="icon active whisListIcon"
                                                        prod-id="<%= productDet[i]._id %>" index="<%= i %>"
                                                        id="wishIcon_<%= i %>">
                                                        <i class="fal fa-heart activei"></i>
                                                    </button>
                                                    <% } else { %>
                                                        <button type="button" class="icon whisListIcon"
                                                            prod-id="<%= productDet[i]._id %>" index="<%= i %>"
                                                            id="wishIcon_<%= i %>">
                                                            <i class="fal fa-heart"></i>
                                                        </button>
                                                        <% } %>
                                            </div>


                                        </div>

                                        <div class="product_image">
                                            <a href="/singleProduct?id=<%=productDet[i]._id %>" class="click_img">
                                                <img src="assets/uploadimages/uploads/<%=productDet[i].productImage[0]%>"
                                                    class="image-fit-contain" alt="img">
                                            </a>
                                        </div>
                                        <div class="product_categories">
                                            <span>
                                                <%=productDet[i].parentCategory.categoryName%>
                                            </span>
                                        </div>
                                        <h4 class="title"><a href="#">
                                                <%=productDet[i].productName%>
                                            </a></h4>
                                        <div class="price">
                                            <span>
                                                &#8377
                                                <%=productDet[i].productPrice%>
                                            </span>
                                            <% if (productDet[i].priceBeforeOffer !==productDet[i].productPrice ) { %>
                                                <span>&#8377&nbsp;<%=productDet[i].priceBeforeOffer%></span>
                                                <% } else { %>
                                                    <% } %>
                                        </div>
                                        <!-- <div class="product_qty"><span>1</span>PC</div> -->
                                        <div class="product_action">
                                            <% if (user) { %>
                                                <button type="button" onclick="addToCart('<%= i %>')"
                                                    class="product_btn">ADD to
                                                    Cart <i class="fal fa-plus"></i></button>
                                                <% } else { %>
                                                    <button type="button" onclick="nonUserAddCart()"
                                                        class="product_btn">ADD to Cart <i
                                                            class="fal fa-plus"></i></button>
                                                    <% } %>

                                        </div>
                                    </div>
                                </div>
                                <% } %>
                                    <!-- Item -->

                                    <div class="row">
                                        <div class="col-12">
                                            <nav>
                                                <ul class="pagination">
                                                    <li class="page-item">
                                                        <a class="page-link" onclick="pageClick('<%= page - 1 %>')"
                                                            aria-label="Previous">
                                                            <span aria-hidden="true">&laquo;</span>
                                                            <span class="sr-only">Previous</span>
                                                        </a>
                                                    </li>
                                                    <% for(let i=1; i <=pages; i++){ %>
                                                        <li class="page-item <%= page == i? 'active' : '' %>"><a
                                                                class="page-link" onclick="pageClick('<%= i %>')">
                                                                <%= i %>
                                                            </a></li>
                                                        <% } %>
                                                            <% if(page==pages){ %>
                                                                <li class="page-item">
                                                                    <a class="page-link"
                                                                        onclick="pageClick('<%= page %>')"
                                                                        aria-label="Next">
                                                                        <span aria-hidden="true">&raquo;</span>
                                                                        <span class="sr-only">Next</span>
                                                                    </a>
                                                                </li>
                                                                <% } else { %>

                                                                    <li class="page-item">
                                                                        <a class="page-link"
                                                                            onclick="pageClick('<%= page + 1 %>')"
                                                                            aria-label="Next">
                                                                            <span aria-hidden="true">&raquo;</span>
                                                                            <span class="sr-only">Next</span>
                                                                        </a>
                                                                    </li>
                                                                    <% } %>
                                                </ul>
                                            </nav>


                                        </div>
                                    </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <!-- Section End -->

    <!-- WishList Success Modal -->
    <div class="modal fade" id="successModal" tabindex="-1" role="dialog" aria-labelledby="successModalLabel"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content" style="background-color: #2ecc709e;">
                <div class="modal-header">
                    <h5 class="modal-title text-white" id="successModalLabel">Success</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
