                            <ul>
                                <li>
                                    <a>Product Categories</a>
                                    <form action="#" method="POST">
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="chair" name="category">
                                                <label class="custom-control-label" for="chair">Chair</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
                                                    id="sofa" name="category">
                                                <label class="custom-control-label" for="sofa">Sofa</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio me-sm-2 form-check">
                                                <input type="radio" class="custom-control-input form-check-input"
