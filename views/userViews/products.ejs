    <script>
        const productDet = '<%- JSON.stringify(productDet) %>'
        const productDetArr = JSON.parse(productDet)
        console.log(productDetArr);
        const filterDet = '<%- JSON.stringify(queryFilters) %>'
        const parsedFilters = JSON.parse(filterDet)
        // console.log(parsedFilters);
        const addToCart = async (index) => {
            console.log(index);
            console.log("Indexed Product :", productDetArr[index]);
            const product = productDetArr[index];
            const prodId = product._id;
            const prodStock = product.productStock;
            const prodQty = 1;
            const prodPrice = product.productPrice;
            console.log("Product Qty: ", prodQty);
            try {
                const response = await fetch(`/addToCart?pid=${prodId}&stock=${prodStock}&quantity=${prodQty}&productPrice=${prodPrice}`, {
                    method: 'POST',
                })
                const data = await response.json();

                if (data.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Product Added to Cart',
                        text: 'The product has been successfully added to your cart.',
                        confirmButtonText: 'OK',
                        confirmButtonColor: '#3085d6',
                    })
                } else {
                    throw new Error('Error adding product to cart.');
                }

            } catch (error) {
                console.log("Error while adding the product to the cart :" + error);
            }

        }

        const page = '<%- JSON.stringify(page) %>'
        const pageVal = JSON.parse(page)
        console.log("Page :", pageVal);
        const pages = '<%- JSON.stringify(pages) %>'
        const pagesVal = JSON.parse(pages)
        console.log("Pages: ", pagesVal);

        //// Retain the filters radio input ////
        const radios = document.querySelectorAll('input[type="radio"]');
        if (parsedFilters) {
            radios.forEach(radio => {
                if (parsedFilters) {
                    for (let key in parsedFilters) {
                        if (parsedFilters[key] === radio.id) {
                            radio.checked = true
                        }
                    }

                }
            });
        }


        const wishListSuccess = new bootstrap.Modal(document.getElementById("successModal"));
        const signupLoginModal = new bootstrap.Modal(document.getElementById("signupLoginModal"));
        const removeSuccessModal = new bootstrap.Modal(document.getElementById("removeSuccessModal"));


        const applyFiltersButton = document.getElementById('apply-filters');
        const removeFiltersButton = document.getElementById('remove-filters');
        async function nonUserAddCart() {
            signupLoginModal.show();
            document
                .getElementById("closeNonUserModal")
                .addEventListener("click", () => {
                    signupLoginModal.hide()
                });
        }

        const selectedFilters = {};
        let url = `/filterProduct?`;


        applyFiltersButton.addEventListener('click', async () => {
            radios.forEach(radio => {
                if (radio.checked) {
                    const val = radio.id;
                    const filterType = radio.name;
                    selectedFilters[filterType] = val;
                }
            });

            const urlParams = new URLSearchParams(window.location.search);
            const searchQuery = urlParams.get('bySearch');

            let url = `/filterProduct?`;

            if (searchQuery) {
                url += `bySearch=${searchQuery}&`;
            }

            Object.keys(selectedFilters).forEach((filterType, index) => {
                url += `${filterType}=${selectedFilters[filterType]}${index < Object.keys(selectedFilters).length - 1 ? '&' : ''}`;
            });

            console.log(url);
            window.location.href = url;
            console.log("Heolloe");
        });
        removeFiltersButton.addEventListener('click', () => {
            window.location.href = '/filterProduct';
            console.log('Filters removed');
        });

        function pageClick(page) {
            const currentPage = parseInt(page);
            const urlParams = new URLSearchParams(window.location.search);
            if (urlParams.has('page')) {
                urlParams.set('page', currentPage);
            } else {
                urlParams.append('page', currentPage);
            }
            window.location.search = urlParams.toString();
        }



        let urlSearch = url; // store the current URL

        function redirectToSearch(event) {
            event.preventDefault();
            const searchQuery = document.querySelector('input[name="searchQuery"]').value;
            const urlParams = new URLSearchParams(window.location.search);
            urlParams.set('bySearch', searchQuery); // update the query string
            const newUrl = `${urlSearch.split('?')[0]}?${urlParams.toString()}`;
            console.log(newUrl); // reconstruct the URL
            window.location.href = newUrl;
        }
        window.addEventListener('load', () => {
            const urlParams = new URLSearchParams(window.location.search);
            const searchQuery = urlParams.get('bySearch');
            if (searchQuery) {
                document.querySelector('input[name="searchQuery"]').value = searchQuery;
            }
        });

        const whisListIcons = document.querySelectorAll('.whisListIcon')

        whisListIcons.forEach(btn => {
            btn.addEventListener('click', async (event) => {
                const productId = event.currentTarget.getAttribute('prod-id')
                const index = event.currentTarget.getAttribute('index')

                if (btn.classList.contains('active') && btn.querySelector('i').classList.contains('activei')) {
                    console.log("active wishlist");
                    try {
                        const response = await fetch(`/removeWishList?productId=${productId}`)
                        const data = await response.json()
                        console.log(data);
                        if (data.success) {
                            removeSuccessModal.show();
                            document
                                .getElementById("removeOkBtn")
                                .addEventListener("click", () => {
                                    removeSuccessModal.hide()
                                });
                            btn.classList.toggle('active');
                            btn.querySelector('i').classList.toggle('activei');

                        }
                    } catch (error) {
                        console.log("error while Attemp to remove that product from the wish list page:" + error);
                    }


                } else {
                    try {
                        console.log("UnActive WishLIst");
                        const response = await fetch(`/wishList?productId=${productId}`)
                        const data = await response.json()
                        if (data.success) {
                            wishListSuccess.show();
                            document
                                .getElementById("okButton")
                                .addEventListener("click", () => {
                                    wishListSuccess.hide()
                                });
                            btn.classList.toggle('active');
                            btn.querySelector('i').classList.toggle('activei');
                        } if (data.notUser) {
                            signupLoginModal.show();
                            document
                                .getElementById("closeNonUserModal")
                                .addEventListener("click", () => {
                                    signupLoginModal.hide()
                                });
                        }
                    } catch (error) {
                        console.log("Error while sending fetch to adding the product to the wishlist: " + error);
                    }
                }
            });
        })

    </script>

