    <style>
        #brand-name {
            font-family: "Bungee Spice", sans-serif;
            font-weight: 400;
            font-style: normal;
            font-size: 30px;
            background-color: #8a5d2a;
        }

        .custom-accordion .card .card-header .btn-link:after {
            display: none;
        }

        div .logoa {
            height: 150px;
            width: 136px;
            padding: 0 0 0 0;
        }


        .image-fit {
            width: 100%;
            object-fit: cover;
            object-position: center;
            height: 19vh;
        }

        .product_block .product_top .icon {
            line-height: 1;
            font-size: 24px;
        }

        .active {
            color: var(--thm-color-four);
        }

        .activei {
            font-weight: 900;
            color: var(--thm-color-four);
        }

        .header .topbar .inner {
            height: 100px;
