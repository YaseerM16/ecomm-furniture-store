                        required: "Please enter a zip code",
                        digits: "Pin Code must be Numeric",
                        minlength: "Zip code must be 6 characters",
                        maxlength: "Zip code must be euqals 6 Digits"
                    },
                    phone: {
                        required: "Please enter a phone number",
                        digits: "Phone number must be Numeric",
                        minlength: "Phone number must be 10 Digits",
                        maxlength: "Phone number must be 10 Digits"
                    },
                    alternateNumber: {
                        digits: "Alternate number must be Numeric",
                        minlength: "Alternate number must be 10 Digits",
                        maxlength: "Alternate number must be 10 Digits",
                    }
                },
                submitHandler: function (form) {
                    const formData = new FormData(editAddressForm);
                    const data = Object.fromEntries(formData.entries());
                    const urlEncodedData = new URLSearchParams(data);
                    const addressId = editAddressForm.address_id.value;

                    // Send HTTP request to the server
                    fetch(`/editAddress?id=${addressId}`, {
                        method: "POST",
                        headers: {
                            "Content-Type": "application/x-www-form-urlencoded",
                        },
                        body: urlEncodedData,
                    })
                        .then(response => {
                            if (response.ok) {
                                const successModal = new bootstrap.Modal(
                                    document.getElementById("successModal")
                                );
                                successModal.show();
                                document
                                    .getElementById("okButton")
                                    .addEventListener("click", () => {
                                        window.location.reload();
                                    });
                                // Show success message or redirect to a new page
                                console.log("Address updated successfully");
                            } else {
                                // Show error message
                                console.error("Failed to update address");
                            }
                            // Handle the edit request here
                            editAddressModal.hide();
                        })
                        .catch(error => {
                            console.error("Error:", error);
                        });
                }
            });
        });

        // delete address

        deleteAddressBtns.forEach(btn => {
            btn.addEventListener("click", () => {
                deleteAddressModal.show();
            })
        })




        document.getElementById('confirm-delete-address').addEventListener('click', async () => {
            const addressId = document.getElementById('delete-address-btn').dataset.addressId;
            const response = await fetch(`/deleteAddress?id=${addressId}`, {
                method: 'DELETE',
            });

            if (response.ok) {
                const result = await response.json();
                if (result.success) {
                    // show success message in modal
                    deleteAddressModal.hide();
                    // document.getElementById('delete-modal-body').innerHTML = 'Address deleted successfully.';
                    const deleteSuccessModal = new bootstrap.Modal(document.getElementById('deleteSuccessModal'));
