                    },
                    addressLine1: {
                        required: true,
                        pattern: /^[a-zA-Z]+(?: [a-zA-Z]+)*$/,
                        minlength: 5
                    },
                    addressLine2: {
                        required: true,
                        pattern: /^[a-zA-Z]+(?: [a-zA-Z]+)*$/,
                        minlength: 5
                    },
                    city: {
                        required: true,
                        pattern: /^[a-zA-Z]+(?: [a-zA-Z]+)*$/,
                        minlength: 4
                    },
                    state: {
                        required: true,
                        pattern: /^[a-zA-Z]+(?: [a-zA-Z]+)*$/,
                        minlength: 4
                    },
