    <style>
        #brand-name {
            font-family: "Bungee Spice", sans-serif;
            font-weight: 400;
            font-style: normal;
            font-size: 30px;
            background-color: #8a5d2a;
        }

        #popupMessageModal {
            /* Add a subtle animation to the modal */
            transition: all 0.3s ease-in-out;
        }

        #popupMessageModal.modal-content {
            /* Add a soft shadow to the modal */
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
            /* Add a subtle border radius */
            border-radius: 10px;
        }

        #popupMessageModal.modal-header {
            /* Add a pleasant background color */
            background-color: #f7f7f7;
            /* Add a subtle border bottom */
            border-bottom: 1px solid #ddd;
        }

        #popupMessageModal.modal-title {
            /* Add a bold font weight */
            font-weight: bold;
            /* Add a pleasant font color */
            color: #333;
        }

        #popupMessageModal.modal-body {
            /* Add some padding to the modal body */
            padding: 20px;
        }

        #popupMessageModal.modal-footer {
            /* Add a subtle border top */
            border-top: 1px solid #ddd;
            /* Add some padding to the modal footer */
            padding: 10px;
        }

        #popupMessageModal.btn {
            /* Add a pleasant border radius */
            border-radius: 5px;
            /* Add a subtle box shadow */
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
        }

        #popupMessageModal.btn-success {
            /* Add a vibrant background color */
            background-color: #8bc34a;
            /* Add a pleasant font color */
            color: #fff;
        }
    </style>
</head>

