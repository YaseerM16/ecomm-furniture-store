                        required: true,
                        minlength: 6,
                        maxlength: 6,
                        digits: true
                    },
                    phone: {
                        required: true,
                        minlength: 10,
                        maxlength: 10,
                        digits: true
                    },
                    alternateNumber: {
                        minlength: 10,
                        maxlength: 10,
                        digits: true
                    }
                },
                messages: {
                    title: {
                        required: "Please enter a title",
                        pattern: "Title should only contain letters and spaces.",
                        minlength: "Title must be at least 4 characters"
                    },
                    firstName: {
                        required: "Please enter a first name",
