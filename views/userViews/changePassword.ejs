    <script>
        $('#myForm').validate({

            rules: {
                username: {
                    pattern: /^[a-zA-Z]+(?: [a-zA-Z]+)*$/,
                    required: true
                },
                email: {
                    pattern: /^[a-zA-Z0-9._%+-]*@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/,
                    required: true

                },
                password: {
                    pattern: /^.{6,30}$/,
                    required: true

                },
                confirmPassword: {
