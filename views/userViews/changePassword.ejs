    <section class="section">
        <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-lg-7">
                    <div class="login_register_box">
                        <div class="image">
                            <img src="assets/images/register.jpg" class="image-fit" alt="img">
                        </div>
                        <div class="box_inner">
                            <h3 class="title">Sign Up to your account</h3>
                            <form id="myForm" action="/changePassword" method="POST">
                                <% if(userExist){ %>
                                    <div class="m-3 py-1 rounded text-center border bg-danger-subtle">
                                        <h3 class="text-danger">User already registered</h3>
                                    </div>
                                    <% } %>
                                        <% if(passwordMismatch){ %>
                                            <div class="m-3 py-1 rounded text-center border bg-danger-subtle">
                                                <h3 class="text-danger">Password and Confirm Password Should be Match
                                                </h3>
                                            </div>
                                            <% } %>
                                                <div class="row">

                                                    <div class="col-12">
                                                        <div class="form-group">
                                                            <label>Password</label>
                                                            <input type="password" name="password"
                                                                class="form-control form-control-custom" id="password"
                                                                style="font-size: 22px;" placeholder="password"
                                                                autocomplete="off" required>
                                                            <% if (errors && errors.length> 0) { %>
                                                                <% errors.forEach(error=> { %>
                                                                    <% if (error.path==='password' ) { %>
                                                                        <div class="alert alert-danger">
                                                                            <%= error.msg %>
                                                                        </div>
                                                                        <% } %>
                                                                            <% }); %>
                                                                                <% } %>

                                                        </div>
                                                    </div>
                                                    <div class="col-12">
                                                        <div class="form-group">
                                                            <label>Confirm Password</label>
                                                            <input type="password" name="confirmPassword"
                                                                style="font-size: 22px;"
                                                                class="form-control form-control-custom"
                                                                id="confirmPassword" placeholder="password"
                                                                autocomplete="off" required>
                                                            <% if (errors && errors.length> 0) { %>
                                                                <% errors.forEach(error=> { %>
                                                                    <% if (error.path==='confirmPassword' ) { %>
                                                                        <div class="alert alert-danger">
                                                                            <%= error.msg %>
                                                                        </div>
                                                                        <% } %>
                                                                            <% }); %>
                                                                                <% } %>

                                                        </div>
                                                    </div>
                                                    <div class="col-12">
                                                        <div class="form-group">
                                                            <div class="custom-control custom-radio me-sm-2 form-check">

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-12">
                                                        <button type="submit"
                                                            class="thm-btn btn-rectangle thm-bg-color-one w-100">submit</button>

                                                    </div>
                                                </div>
                            </form>
                        </div>
