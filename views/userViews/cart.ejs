    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
    <script>
        const successModal = new bootstrap.Modal(document.getElementById("successModal"));
        const removeFromCartModal = new bootstrap.Modal(document.getElementById("removeFromCartModal"));
        const cartProducts = '<%- JSON.stringify(cartProducts) %>'
        const cartPdArr = JSON.parse(cartProducts)
        console.log(cartPdArr);
        // const presentQty = cartPdArr[0].productQuantity

        // console.log(presentQty);
        // const parsedGrandSum = parseInt(grandSum.innerHTML)
        // console.log(parsedGrandSum);
        let grandTotal = 0;
        for (let i = 0; i < cartPdArr.length; i++) {
            let totalCostPer = document.getElementById(`totalCostPerProduct${cartPdArr[i].productId._id}`)
            let quantity = document.getElementById(`productQuantity${cartPdArr[i].productId._id}`).value
            let productPrice = cartPdArr[i].productId.productPrice
            let totalCostPerVal = quantity * productPrice
            grandTotal += cartPdArr[i].totalCostPerProduct
            totalCostPer.innerHTML = totalCostPerVal
            // console.log(grandTotal);
        }

        document.getElementById('grand-total').innerHTML = grandTotal
        // grandTotal += cartPdArr[i].totalCostPerProduct
        // grandSum.innerHTML = parsedGrandSum + grandTotal

        async function removeFromCart(productId, userId) {
            removeFromCartModal.show()

            document.getElementById("cancel-remove-btn").addEventListener("click", function () {
                removeFromCartModal.hide()
            })

            document.getElementById("confirm-remove-from-cart").addEventListener("click", async function () {
                try {

                    const response = await fetch(`/removePdCart?productID=${productId}&userID=${userId}`, {
                        method: "GET",
                    });

                    const data = await response.json();
                    if (data.success) {
                        removeFromCartModal.hide()
                        successModal.show()
                        document.getElementById("btn-success").addEventListener("click", function () {
                            successModal.hide()
                            window.location.reload();
                        })
                        console.log("Item was Removed from the Cart ::");
                    } else {
                        console.log("THe Opeeration was Unsuccessfull:: success is not true");
                    }

                } catch (error) {
                    console.log("Error while Attemp to Remove the Product from the Cart: " + error);
                }
            })

        }



        document.getElementById('proceed-to-checkout').addEventListener("click", () => {
            const cartData = [];
            document.querySelectorAll(".dataClass").forEach(ele => {
                const obj = {
                    productId: ele.getAttribute('data-pid'),
                    quantity: parseInt(ele.value)
                };
                cartData.push(obj);
            })
            const form = document.createElement('form');
            const grndTt = parseInt(document.getElementById('grand-total').innerHTML);
            form.method = 'POST';
            form.action = `/selectAddress?grandTotal=${grndTt}`;

            const input = document.createElement('input');
            input.type = 'hidden';
