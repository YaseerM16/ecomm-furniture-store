            let inputValue = document.getElementById(`productQuantity${productId}`).value;
            const productPrice = cartPdArr[index].productId.productPrice

            let originalValue = inputValue;
            try {


                const response = await fetch(`/cartIncBtn?action=${action}&productID=${productId}&quantity=${inputValue}`, {
                    method: "GET",
                });

                const data = await response.json();
                if (data.success) {

                    // console.log(data.quantity);
                    // console.log(data.success);
                    // const c = parseInt(data.quantity)
                    // console.log(c);
                    // console.log(data.totalCostPerProduct);
                    const grandSum = document.getElementById('grand-total')
                    let grandSumVal = parseInt(grandSum.innerHTML)
                    document.getElementById(`productQuantity${productId}`).value = data.quantity

                    let totalCostPerProductVal = parseInt(data.quantity) * productPrice
                    totalCostPerProduct.innerHTML = totalCostPerProductVal
                    // console.log(totalCostPerProductVal)
                    // console.log(data.totalCostPerProduct);
                    if (data.action == "plus") {
                        grandSumVal += productPrice
                        grandSum.innerHTML = grandSumVal
                    } else if (data.action == "minus") {
                        grandSumVal -= productPrice
                        grandSum.innerHTML = grandSumVal
                    }
                    console.log("Sending and coming");
                }

                if (data.exceed) {
                    const outOfStockModal = new bootstrap.Modal(
                        document.getElementById("outOfStockModal")
                    )
                    outOfStockModal.show()

                    document.getElementById(`productQuantity${productId}`).value = originalValue
                    document
                        .getElementById("close-btn")
                        .addEventListener("click", () => {
                            outOfStockModal.hide();
                        });
                }
            } catch (err) {
                console.log("The /cartIncBtn interrupted with an error:", err);
