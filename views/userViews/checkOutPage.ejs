    <style>
        #brand-name {
            font-family: "Bungee Spice", sans-serif;
            font-weight: 400;
            font-style: normal;
            font-size: 30px;
            background-color: #8a5d2a;
        }

        .alert-vibrant {
            background-color: #ff7675;
            color: white;
            border: 2px solid #d63031;
            border-radius: 10px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        }

        .btn-contact-support {
            background-color: #676767;
            border-color: #ff7675;
            color: white;
            transition: background-color 0.3s ease, border-color 0.3s ease;
        }

        .btn-contact-support:hover {
            background-color: #d63031;
            border-color: #d63031;
        }

        .applied-coupon {
            background-color: #048e016d;
            /* Bootstrap warning color */
            border: 2px solid #343a40;
            /* Bootstrap dark color */
            border-radius: .25rem;
            padding: 1rem;
            margin-bottom: 1rem;
            position: relative;

        }

        .applied-coupon .coupon-code {
            font-weight: bold;
            color: #007bff;
            /* Bootstrap primary color */
        }

        .applied-coupon .discount-badge {
            background-color: #28a745;
            /* Bootstrap success color */
            color: #0c0c0c;
            padding: .25em .5em;
            border-radius: .25rem;
        }

        .applied-coupon .minimum-purchase {
            color: #6c757d;
            /* Bootstrap secondary color */
            display: block;
            margin-top: .5rem;
        }

        .applied-coupon .minimum-purchase strong {
            color: #343a40;
            /* Bootstrap dark color */
        }

        .applied-coupon .remove-btn {
            position: absolute;
            top: .5rem;
            right: .5rem;
            background-color: #dc3545;
            /* Bootstrap danger color */
            color: #fff;
            border: none;
            padding: .25rem .5rem;
            font-size: .875rem;
            cursor: pointer;
        }
    </style>
</head>

