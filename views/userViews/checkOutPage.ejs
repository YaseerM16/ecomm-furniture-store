                        <script>
                            const cartProducts = '<%- JSON.stringify(cartProducts) %>'
                            const cartTotalVal = '<%- JSON.stringify(grandTotal) %>'
                            const couponData = '<%- JSON.stringify(couponDet) %>'
                            const couponsData = '<%- JSON.stringify(coupons) %>'
                            const cartTotal = JSON.parse(cartTotalVal)
                            const couponDet = JSON.parse(couponData)
                            const couponsDet = JSON.parse(couponsData)
                            const parsedCartTot = parseInt(cartTotal)
                            const cartPdArr = JSON.parse(cartProducts)
                            console.log(cartPdArr);
                            console.log(couponDet);

                            async function couponFunc(couponId) {
                                try {

                                    const response = await fetch(`/applyCoupon?couponID=${couponId}&grandTotal=${parsedCartTot}`, {
                                        method: 'GET',
                                    });
                                    const result = await response.json();
                                    console.log(result);

                                    if (result.couponCofirmed) {
                                        Swal.fire({
                                            icon: "success",
                                            title: "Your coupon has been Applied",
                                            showConfirmButton: true,
                                        })
                                            .then((result) => {
                                                if (result.isConfirmed) {
                                                    window.location.reload()
                                                }
                                            })
                                    } else if (result.couponCodeExists) {
                                        await Swal.fire({
                                            icon: "error",
                                            title: "Coupon Code Applied Already",
                                            text: "Coupon Code must be Added Once"
                                        });
                                    } else if (result.notEligible) {
                                        await Swal.fire({
                                            icon: "warning",
                                            iconHtml: '<i class="fas fa-exclamation-triangle"></i>',
                                            title: "Insufficient Purchase Amount",
                                            text: "Your purchase amount is less than the minimum required to apply the coupon.",
                                            customClass: {
                                                icon: 'swal2-warning-icon'
                                            }
                                        });
                                    }
                                } catch (error) {
                                    console.log("Error while sending the apply coupon request :" + error);
                                }



                            }
                            async function removeCoupon(event, couponId) {
                                try {
                                    event.stopImmediatePropagation();
                                    event.preventDefault();
                                    await Swal.fire({
                                        icon: "Confirmation",
                                        title: "Are you sure to Remove Coupon",
                                        showConfirmButton: true,
                                        showCancelButton: true,
                                        confirmButtonText: 'OK',
                                        cancelButtonText: 'Dismiss'
                                    })
                                        .then(async (result) => {
                                            if (result.isConfirmed) {
                                                const response = await fetch(`/removeCoupon?couponID=${couponId}`, {
                                                    method: 'GET',
                                                });
                                                const result = await response.json();

                                                if (result.isRemoved) {
                                                    Swal.fire({
                                                        icon: "success",
                                                        title: "Your coupon has been Removed",
                                                        showConfirmButton: true,
                                                    })
                                                        .then((result) => {
                                                            if (result.isConfirmed) {
                                                                window.location.reload()
                                                            }
                                                        })
                                                }

                                            } else if (result.couponNotFound) {
                                                Swal.fire({
                                                    icon: "error",
                                                    title: "The coupon trying to Remove is not found",
                                                    showConfirmButton: true,
                                                })
                                            }
                                        })
                                }
                                catch (error) {
                                    console.log("Error while sending the apply coupon request :" + error);
                                }
                            }

                        </script>
