        document.addEventListener('DOMContentLoaded', function () {

            const returnBtns = document.querySelectorAll('.returnBtn');
            returnBtns.forEach(btn => {

                const orderDateStr = btn.getAttribute('data-order-date');
                console.log(orderDateStr);
                const orderDate = new Date(orderDateStr);
                const currentDate = new Date();
                console.log(currentDate)
                console.log(orderDate)
                const timeDiff = currentDate - orderDate;
                // Convert to days
                const dayDiff = timeDiff / (1000 * 3600 * 24);
                if (dayDiff > 14) {
                    btn.style.display = 'none';
                }
            })
            // Calculate the difference in milliseconds

            // Hide the button if more than 14 days have passed
        });

        document.querySelectorAll('#singleProCancel').forEach(btn => {
            btn.addEventListener("click", (event) => {
                const prodId = event.target.getAttribute("data-pid")
                const orderId = event.target.getAttribute("data-rid")
                console.log(prodId);
                cancelOrderModal.show();
                document.getElementById('cancelConfirm').addEventListener("click", async () => {
                    cancelOrderModal.hide()
                    const response = await fetch(`/singleProdCancel?cartProdId=${prodId}&orderId=${orderId}`, {
                        method: "DELETE",
                    });

                    const data = await response.json();
                    if (data.success) {
                        successModal.show()
                        document.getElementById('success-btn').addEventListener("click", () => {
                            successModal.hide();
                            window.location.reload()
                        })
                    }
                    if (data.failed) {
                        console.log("There is Failed to cancel that product form user side: ");
                    }

                })


            })
        })
        async function returnOrder(cartId, orderId) {
            const { value: returnReason } = await Swal.fire({
                title: 'Return Reason',
                input: 'text',
                inputLabel: 'Please provide a reason for returning',
                inputPlaceholder: 'Enter your reason here...',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                cancelButtonText: 'Cancel',
                confirmButtonText: 'Submit'
            });

            if (returnReason) {

                Swal.fire({
                    title: "Are you sure?",
                    icon: "warning",
                    showCancelButton: true,
                    confirmButtonColor: "#3085d6",
                    cancelButtonColor: "#d33",
                    confirmButtonText: "Yes, Return this order!"
                }).then(async (result) => {
                    if (result.isConfirmed) {
                        let response = await fetch(`/user/returnOrder?cartId=${cartId}&orderId=${orderId}&reason=${returnReason}`, {
                            method: 'POST'
                        })
                        let result = await response.json()
                        await Swal.fire({

                            icon: "success",
                            title: "Order Returned",
                            showConfirmButton: false,
                            timer: 1500
                        });

                        if (result.success) {
                            window.location.reload()
                        }
                    }
                });
            }
        }

