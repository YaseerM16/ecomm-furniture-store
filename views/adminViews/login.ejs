                                <div class="input-group-append input-group-text">
                                    <button type="submit">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                            <!-- Search -->
                        </form>
                    </div>
                    <!-- Search Form -->
                    <!-- Header Action -->
                    <div class="header_action">
                        <ul>
                            <li>
                                <a href="/wishListPage">
                                    <i class="far fa-heart"></i>
                                </a>
                            </li>
                            <li class="cart">
                                <a href="/cartPage">
                                    <span class="cart_value">0</span>
                                    <i class="far fa-shopping-cart"></i>
                                    <div class="cart_text">
                                        My Cart
                                        <!-- <span class="amount">$150.00</span> -->
                                    </div>
                                </a>
                            </li>
                            <li class="user">
                                <% if (user) { %>
                                    <a href="/userDetails">
                                        <img src="assets/images/header/user.png" class="image-fit" alt="img">
                                        <span>
                                            <%= user.username %>
                                        </span>
                                    </a>
                                    <ul class="sub-menu">
                                        <li>
                                            <a href="/adimDashBoard">Dashboard</a>
                                        </li>
                                        <li>
                                            <a href="/logout">Logout</a>
                                        </li>
                                    </ul>
                                    <% } else { %>
                                        <a href="account.html">
                                            <img src="assets/images/header/user2.png" class="image-fit" alt="img">
                                            <span>Login as Admin</span>
                                        </a>
                                        <ul class="sub-menu">
                                            <li>
                                                <a href="/adminLogin">Admin Login</a>
                                            </li>
                                        </ul>
                                        <% } %>
                            </li>
                        </ul>
                    </div>
                    <!-- Header Action -->
                </div>
            </div>
        </div>
        <!-- Topbar -->
    </header>
    <!-- Header End -->

    <!-- Section Start -->
    <section class="section">
        <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-lg-7">
                    <div class="login_register_box">

                        <div class="box_inner">
                            <h3 class="title">Log in as Admin</h3>
                            <form id="myForm" action="/adminLogin" method="POST">
                                <% if(invalid){ %>
                                    <div class="m-3 py-1 rounded text-center border bg-danger-subtle">
                                        <h3 class="text-danger">Invalid Credentials!!</h3>
                                    </div>
                                    <% } %>
                                        <div class="row">
                                            <div class="col-12">
                                                <div class="form-group">
                                                    <label>Email/username</label>
                                                    <input type="email" name="email" style="font-size: 22px;"
                                                        class="form-control form-control-custom" placeholder="Email I'd"
                                                        autocomplete="off" required>
                                                    <% if (errors && errors.length> 0) { %>
                                                        <% errors.forEach(error=> { %>
                                                            <% if (error.path==='email' ) { %>
                                                                <div class="alert alert-danger">
                                                                    <%= error.msg %>
                                                                </div>
                                                                <% } %>
                                                                    <% }); %>
                                                                        <% } %>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <div class="form-group">
                                                    <input type="password" name="password" style="font-size: 22px;"
                                                        class="form-control form-control-custom" id="password_value"
                                                        placeholder="password" autocomplete="off" required>
                                                    <% if (errors && errors.length> 0) { %>
                                                        <% errors.forEach(error=> { %>
                                                            <% if (error.path==='password' ) { %>
                                                                <div class="alert alert-danger">
                                                                    <%= error.msg %>
                                                                </div>
                                                                <% } %>
                                                                    <% }); %>
                                                                        <% } %>

                                                </div>
                                            </div>

                                            <div class="col-12">
                                                <button type="submit"
                                                    class="thm-btn btn-rectangle thm-bg-color-one w-100">Login</button>
                                            </div>
                                        </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
