    <script>
        const product = '<%- JSON.stringify(productDet) %>'
        const parsePro = JSON.parse(product)
        const cate = '<%- JSON.stringify(categoryDet) %>'
        const parseCat = JSON.parse(cate)
        console.log(parseCat);
        var cropperImage = document.getElementById('cropperImage');
        var croppedImageInput = document.getElementById('croppedImageInput');
        console.log(document.getElementById('image1'));
        const croppedImages = [];

        document.addEventListener('DOMContentLoaded', function () {
            var image = document.getElementById('image');
            var cropper;

            document.getElementById('inputImage').addEventListener('change', function (e) {
                const previewDiv = document.getElementById('image-preview-container');
                previewDiv.style.display = 'block';

                var files = e.target.files;
                var done = function (url) {
                    image.src = url;
                    // Check if cropper is already initialized, destroy it before reinitializing with the new image
                    if (cropper) {
                        cropper.destroy();
                    }
                    cropper = new Cropper(image, {
                        aspectRatio: NaN,
                        viewMode: 2,
                        autoCropArea: 1,
                        cropBoxResizable: false,
                    });
                };
                var reader;
                var file;
                var url;

                if (files && files.length > 0) {
                    file = files[0];
                    if (URL) {
                        done(URL.createObjectURL(file));
                    } else if (FileReader) {
                        reader = new FileReader();
                        reader.onload = function (e) {
                            done(reader.result);
                        };
                        reader.readAsDataURL(file);
                    }
                }
            });

            //...



            // Move the crop functionality inside the DOMContentLoaded event

            document.getElementById('cropButton').addEventListener('click', async (event) => {
                event.preventDefault();


                if (typeof cropper !== 'undefined' && cropper !== null) {
                    const croppedCanvas = cropper.getCroppedCanvas();
                    if (croppedCanvas !== null) {
                        const croppedImageBase64 = croppedCanvas.toDataURL('image/jpeg');
                        const blob = await fetch(croppedImageBase64).then(response => response.blob());
                        const imageUrl = URL.createObjectURL(blob);

                        const imgElement = document.createElement('img');
                        imgElement.src = imageUrl;

                        const deleteButton = document.createElement('button');
                        deleteButton.textContent = 'Drop';
                        deleteButton.className = 'btn btn-sm btn-danger';
                        deleteButton.style.marginTop = '10px';
                        deleteButton.addEventListener('click', () => {
                            imgElement.parentNode.removeChild(imgElement);
                            deleteButton.parentNode.removeChild(deleteButton);
                            const index = croppedImages.indexOf(croppedImageBase64);
                            console.log(index);
                            if (index !== -1) {
                                console.log(index);
                                croppedImages.splice(index, 1);
                            }
                            croppedImageInput.value = JSON.stringify(croppedImages);
                        });

                        const container = document.createElement('div');
                        container.style.display = 'flex';
                        container.style.flexDirection = 'column';
                        container.style.alignItems = 'center';
                        // container.style.justifyContent = 'space-between';
                        container.style.width = '200px';
                        container.style.height = '200px';
                        container.appendChild(imgElement);
                        container.appendChild(deleteButton);

                        document.getElementById('imageContainer').appendChild(container);

                        // Convert blob URL to base64-encoded string
                        const reader = new FileReader();
                        reader.readAsDataURL(blob);
                        reader.onloadend = () => {
                            const base64String = reader.result;
                            croppedImages.push(base64String);
                            croppedImageInput.value = JSON.stringify(croppedImages);
                        };
                    } else {
                        console.error('Error: Cropped canvas is null.');
                    }
                } else {
                    console.error('Error: Cropper is undefined or null.');
                }
            });


        });





        function dataURLToBlob(dataURL) {
            if (dataURL) {
                const base64String = dataURL.split(',')[1];
                if (base64String) {
                    const base64StringWithoutWhitespace = base64String.replace(/\s/g, '');
                    const blob = new Blob([atob(base64StringWithoutWhitespace)], { type: 'image/jpeg' });
                    return blob;
                } else {
                    console.error('base64String is undefined');
                    return null;
                }
            } else {
                console.error('dataURL is undefined');
                return null;
            }
        }





        $('#editProduct').validate({
            rules: {
                productName: {
                    required: true,
                },
                productPrice: {
                    required: true,
                    pattern: "[1-9]\\d*"
                },
                productStock: {
                    required: true,
                    pattern: "0|[1-9][0-9]*"
                },
                parentCategory: {
                    required: true,

                },
                images: {
                    required: true,
                }
            },

            submitHandler: async (form, event) => {

                event.preventDefault()
                // Create a new FormData object
                const formData = new FormData(form);
                console.log(croppedImages);
                croppedImages.forEach((imageBase64, index) => {
                    const blob = dataURItoBlob(imageBase64);
                    formData.append(`croppedImage_${index}`, blob, `image_${index}.jpg`);
                });

                function dataURItoBlob(dataURI) {
                    const byteString = atob(dataURI.split(',')[1]);
                    const mimeString = dataURI.split(',')[0].split(':')[1].split(';')[0];
                    const ab = new ArrayBuffer(byteString.length);
                    const ia = new Uint8Array(ab);
                    for (let i = 0; i < byteString.length; i++) {
                        ia[i] = byteString.charCodeAt(i);
                    }
                    return new Blob([ab], { type: mimeString });
                }


                console.log(formData);



                let response = await fetch('/admin/productManagement/editProduct/<%=productDet._id%>', {
                    method: 'POST',
                    body: formData
                })
                let result = await response.json()
                if (result.exists) {
                    await Swal.fire({
                        icon: "error",
                        title: "Product Already exists"
                    })
                }
                if (result.success) {
                    await Swal.fire({
                        icon: "success",
                        title: "Product Edited"
                    })
                    window.location.href = '/admin/productManagement/productList'
                }
                if (result.noValue) {
                    await Swal.fire({
                        icon: "error",
                        title: "No Value in Product Name"
                    })
                }
            }
        })
        // Handle delete button click event
        $('.delete-image').click(function () {
            // Get the product id and index of the image to be deleted
            var productId = $(this).data('id');
            var index = $(this).data('index');

            // Send an AJAX request to delete the image
            $.ajax({
                url: '/admin/editProduct/delete-image',
                method: 'POST',
                data: {
                    productId: productId,
                    index: index
                },
                success: function (response) {
                    // Remove the image element from the DOM
                    console.log("image deleted : index" + index);
                    console.log($('#image' + index));
                    $('#image' + index).fadeOut(300, function () {
                        console.log("Image faded out");
                        $(this).remove();
                    });
                }
            });
        });


    </script>
