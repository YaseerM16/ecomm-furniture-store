    <style>
        #brand-name {
            font-family: "Bungee Spice", sans-serif;
            font-weight: 400;
            font-style: normal;
            font-size: 30px;
            background-color: #8a5d2a;
        }

        div .logoa {
            height: 150px;
            width: 136px;
            padding: 0 0 0 0;
        }

        .inner {
            height: 19vh;
        }

        .image-fit {
            width: 100%;
            object-fit: cover;
            object-position: center;
            height: 19vh;
        }

        .order_info {
            display: flex;
            flex-direction: column;
        }

        /* Default styles for all viewport widths */
        .order_list_block {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-around;

        }

        @media (min-width: 992px) {
            .navbar-expand-lg .navbar-nav {
                flex-direction: row;
                justify-content: space-around;
            }
        }


        /* .order_list_block .image_info_box {
            width: 30%;
        }

        .order_list_block .order_info {
            width: 20%;
        } */

        /* Styles for viewport widths less than 768px */
        @media (max-width: 767.98px) {
            .order_list_block .image_info_box {
                width: 100%;
            }

            .order_list_block .order_info {
                width: 50%;
            }

            .order_list_block .price {
                display: flex;
                justify-content: center;
            }
        }

        /* Styles for viewport widths less than 576px */
        @media (max-width: 575.98px) {
            .order_list_block .order_info {
                width: 100%;
            }
        }

        .h-100 {
            height: 100%;
            /* Occupy full height of parent container */
            display: flex;
            flex-direction: column;
        }

        .navbar {
            flex-grow: 1;
            /* Allow the navbar to grow and occupy available space */
        }

        .navbar-nav {
            flex-direction: column;
            /* Align nav items vertically */
        }

        .product-offers-button {
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: large;
            background-color: #8a5d2a;
            color: #ffffff;
            padding: 8px;
            border-radius: 10px;
            box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
            cursor: pointer;
            transition: background-color 0.3s ease-in-out;
        }

        .product-offers-button:hover {
            background-color: #ff564e;
        }

        #user .iconx {
            background-color: #604220;
            color: #ffffff;
            border-radius: 50%;
            width: 50px;
            height: 50px;
            font-size: 24px;
            margin: 0 auto;
            /* Center the icon horizontally */
            display: flex;
            align-items: center;
            justify-content: center;
            transition:
                background-color 0.7s ease,
                color 0.7s ease,
                transform 0.7s ease,
                width 0.7s ease,
                height 0.7s ease,
                font-size 0.7s ease;
        }

        #user:hover .iconx {
            background-color: #C7A379;
            color: #ffffff;
            transform: scale(1.6);
            /* No margin change to prevent layout shift */
        }

        .header .topbar .inner {
            height: 100px;
        }
    </style>
</head>

<body>

    <!-- Header Start -->
    <header class="header">
        <!-- Topbar -->
        <div class="topbar">
